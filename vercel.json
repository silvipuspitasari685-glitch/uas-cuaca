{
    "version": 2,
    "builds": [
        { "src": "Api/getWeather.js", "use": "@vercel/node" },
        { "src": "static/**", "use": "@vercel/static-build" } 
    ],
    "routes": [
        { "src": "/api/getWeather", "dest": "/Api/getWeather.js" },
        { "src": "/(.*)", "dest": "/static/$1" }
    ]
}